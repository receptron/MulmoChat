{
  "name": "MulmoChat",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "author": "snakajima",
  "license": "AGPL-3.0-only",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "kill": "pkill -f 'tsx server/index.ts' && pkill -f 'vite'",
    "route": "sudo route delete -net 192.168.1",
    "dev:client": "vite",
    "dev:server": "tsx server/index.ts",
    "server": "tsx server/index.ts",
    "build": "vite build && npm run build:server",
    "build:client": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "build:server": "tsc -p server/tsconfig.json",
    "lint": "eslint src server",
    "preview": "vite preview",
    "start": "NODE_ENV=production node dist/server/index.js",
    "format": "prettier --write '{src,server}/**/*.{ts,json,yaml,vue}'",
    "latest": "yarn upgrade-interactive  --latest",
    "test:text:openai": "tsx server/tests/test-text-openai.ts",
    "test:text:anthropic": "tsx server/tests/test-text-anthropic.ts",
    "test:text:google": "tsx server/tests/test-text-google.ts",
    "test:text:ollama": "tsx server/tests/test-text-ollama.ts",
    "test:tools:openai": "tsx server/tests/test-tools-openai.ts",
    "test:tools:anthropic": "tsx server/tests/test-tools-anthropic.ts",
    "test:tools:google": "tsx server/tests/test-tools-google.ts",
    "test:tools:ollama": "tsx server/tests/test-tools-ollama.ts",
    "test:image:comfy": "tsx server/tests/test-comfyui.ts",
    "test:image:openai": "tsx server/tests/test-image-openai.ts",
    "benchmark:llm": "tsx benchmark/src/cli.ts",
    "benchmark:test": "tsx benchmark/src/test-runner.ts",
    "benchmark:check": "tsx benchmark/src/cli.ts check",
    "benchmark:list": "tsx benchmark/src/cli.ts list",
    "benchmark:re-evaluate": "tsx benchmark/src/re-evaluate.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@google/genai": "^1.38.0",
    "@gui-chat-plugin/browse": "^0.1.0",
    "@gui-chat-plugin/camera": "^0.1.0",
    "@gui-chat-plugin/canvas": "^0.1.0",
    "@gui-chat-plugin/edit-html": "^0.1.0",
    "@gui-chat-plugin/edit-image": "^0.0.1",
    "@gui-chat-plugin/exa": "^0.1.1",
    "@gui-chat-plugin/generate-html": "^0.1.0",
    "@gui-chat-plugin/go": "^0.1.0",
    "@gui-chat-plugin/html": "^0.1.0",
    "@gui-chat-plugin/map": "^0.1.0",
    "@gui-chat-plugin/markdown": "^0.0.1",
    "@gui-chat-plugin/mulmocast": "^0.0.2",
    "@gui-chat-plugin/music": "^0.0.1",
    "@gui-chat-plugin/othello": "^0.2.0",
    "@gui-chat-plugin/present3d": "^0.0.2",
    "@gui-chat-plugin/scroll-to-anchor": "^0.1.0",
    "@gui-chat-plugin/set-image-style": "^0.1.0",
    "@gui-chat-plugin/spreadsheet": "^0.2.0",
    "@gui-chat-plugin/switch-role": "^0.2.0",
    "@gui-chat-plugin/text-response": "^0.1.0",
    "@gui-chat-plugin/tictactoe": "^0.1.0",
    "@gui-chat-plugin/todo": "^0.1.0",
    "@gui-chat-plugin/weather": "^0.0.1",
    "@mulmochat-plugin/form": "^0.3.0",
    "@mulmochat-plugin/generate-image": "^0.3.0",
    "@mulmochat-plugin/quiz": "^0.3.0",
    "@mulmochat-plugin/summarize-pdf": "^0.3.0",
    "@mulmochat-plugin/ui-image": "^0.1.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.2",
    "exa-js": "^2.1.1",
    "express": "^5.2.1",
    "guichat-plugin-akinator": "github:isamu/AkinatorGame",
    "material-icons": "^1.13.14",
    "uuid": "^13.0.0",
    "vue": "^3.5.27",
    "vue-drawing-canvas": "^1.0.14",
    "vue-router": "^4.6.4",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.10",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
