    "eslint-plugin-sonarjs": "^3.0.5",
    "postcss": "^8.4.38",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vue-tsc": "^3.2.2"
  }
}
